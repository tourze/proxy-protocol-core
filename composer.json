{
  "name": "tourze/proxy-protocol-core",
  "description": "A PHP core library for parsing and generating Proxy Protocol. Supports both V1 (text format) and V2 (binary format) versions.",
  "type": "library",
  "license": "MIT",
  "require": {
    "tourze/enum-extra": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\ProxyProtocol\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\ProxyProtocol\\Tests\\": "tests/"
    }
  }
}